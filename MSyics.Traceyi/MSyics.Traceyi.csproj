<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <SignAssembly>false</SignAssembly>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>2.6.0</Version>
    <PackageId>Traceyi</PackageId>
    <Authors>MSyics</Authors>
    <Company>MSyics</Company>
    <Product>Traceyi</Product>
    <PackageProjectUrl>https://github.com/MSyics/Traceyi</PackageProjectUrl>
    <RepositoryUrl>https://github.com/MSyics/Traceyi</RepositoryUrl>
    <PackageLicenseUrl></PackageLicenseUrl>
    <Copyright>Copyright (c) 2018 MSyics</Copyright>
    <Description>Traceyi is an asynchronous logging library for .NET Standard.</Description>
    <Platforms>AnyCPU;x64</Platforms>
    <PackageTags>logging trace log async</PackageTags>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DocumentationFile>bin\Release\netstandard2.0\MSyics.Traceyi.xml</DocumentationFile>
    <NoWarn>1701;1702;1705,1591</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Configuration\**" />
    <Compile Remove="obsolete_Configuration\**" />
    <Compile Remove="x_Configuration\**" />
    <Compile Remove="_Obsolete\**" />
    <EmbeddedResource Remove="Configuration\**" />
    <EmbeddedResource Remove="obsolete_Configuration\**" />
    <EmbeddedResource Remove="x_Configuration\**" />
    <EmbeddedResource Remove="_Obsolete\**" />
    <None Remove="Configuration\**" />
    <None Remove="obsolete_Configuration\**" />
    <None Remove="x_Configuration\**" />
    <None Remove="_Obsolete\**" />
  </ItemGroup>

  <ItemGroup>
    <Compile Remove="Listeners\EventLog.cs" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="App.config" />
  </ItemGroup>

  <ItemGroup>
    <None Include="Layout\LogLayout.cs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="microsoft.Extensions.Configuration" Version="2.2.0" />
    <PackageReference Include="Microsoft.Extensions.Configuration.Binder" Version="2.2.4" />
    <PackageReference Include="microsoft.Extensions.Configuration.Json" Version="2.2.0" />
  </ItemGroup>

  <ItemGroup>
    <None Update="Traceyi.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>

  <ProjectExtensions><VisualStudio><UserProperties Traceyi_1json__JSONSchema="http://json.schemastore.org/config" /></VisualStudio></ProjectExtensions>

</Project>
