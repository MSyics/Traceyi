{
  "Traceyi": {
    "Tracer": [
      {
        "Listeners": [ "db", "1console" ],
        "Filters": "all"
      }
    ],
    "Listener": {
      "UsingDatabase": [
        {
          "Name": "db",
          "UseAsync": true,
          "UseLock": false,
          "Demux": 4,
          "Chunk": 100,
          "ChunkTimeout": "0:0:1.00"
        }
      ],
      "Console": [
        {
          "Name": "console"
        }
      ],
      "File": [
        {
          "Name": "file",
          "Demux": 1,
          "UseLock": false,
          "UseAsync": true,
          "UseMutex": false,
          "KeepFilesOpen": true,
          "MaxLength": 25000000,
          "MaxArchiveCount": 0,
          "Path": "app.log"
        }
      ]
    }
  }
}
