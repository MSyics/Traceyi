{
  "Traceyi": {
    "Tracer": [
      {
        "Listeners": [ "console" ],
        "Filters": "all"
      }
    ],
    "Listener": {
      "Console": [
        {
          "Name": "console",
          "Layout": {
            "Format": "{action| ,8:L} {traced} {@=>json}"
          }
        }
      ]
    }
  }
}
