{
  "Traceyi": {
    "Tracer": [
      {
        "Listeners": [ "console" ],
        "Filters": "all"
      }
    ],
    "Listener": {
      "Console": [
        {
          "Name": "console",
          "Coloring": {
            "start": 0,
            "length": 3
          },
          "Layout": {
            "Format": "{@[action,traced,elapsed]=>json}"
          }
        }
      ]
    }
  }
}
