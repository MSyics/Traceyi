{
  "Traceyi": {
    "Tracer": [
      {
        "Listeners": [ "console" ],
        "Filters": "all"
      }
    ],
    "Listener": {
      "Console": [
        {
          "Name": "console",
          "UseLock": false,
          "UseAsync": true,
          "Encoding": "utf-8",
          "coloring": {
            "start": 0,
            "length": 10
          },
          "Layout": {
            "format": "{@=>json,indent}"
          }
        }
      ]
    }
  }
}
