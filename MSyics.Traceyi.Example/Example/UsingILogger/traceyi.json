{
  "Traceyi": {
    "Tracer": [
      {
        "Listeners": [ "console", "console22", "file" ],
        "Filters": "all"
      }
    ],
    "Listener": {
      "Console": [
        {
          "Name": "console",
          "UseLock": false,
          "UseAsync": true,
          "Encoding": "utf-8",
          "coloring": {
            "start": 0,
            "length": 1
          },
          "Layout": {
            "format": "----------------{newLine}{activityId=>json}{newline}{extensions=>json,indent}{newline}{@[all]=>json,indent}"
          }
        },
        {
          "Name": "console2",
          "UseLock": true,
          "UseAsync": true,
          "Encoding": "utf-8",
          "Layout": {
            "Format": "{@=>json,indent}"
          }
        }
      ],
      "File": [
        {
          "Name": "file",
          "UseLock": false,
          "UseAsync": true,
          "UseMutex": false,
          "KeepFilesOpen": true,
          "MaxLength": 25000000,
          "MaxArchiveCount": 1,
          "Path": "UsingILogger\\app_{processName}.log",
          "Layout": {
            "format": "{action}{tab}{@[message]=>json,indent}"
          }
        }
      ]
    }
  }
}
