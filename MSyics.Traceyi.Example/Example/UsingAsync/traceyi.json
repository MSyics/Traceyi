{
  "Traceyi": {
    "Tracer": [
      {
        "Listeners": [ "console2", "file" ],
        "Filters": "all"
      }
    ],
    "Listener": {
      "Console": [
        {
          "Name": "console",
          "Concurrency": 30,
          "UseLock": true,
          "UseAsync": true
        }
      ],
      "File": [
        {
          "Name": "file",
          "Concurrency": 1,
          "UseLock": false,
          "UseAsync": true,
          "UseMutex": false,
          "KeepFilesOpen": true,
          "MaxLength": 25000000,
          "MaxArchiveCount": 1,
          //"Path": "app_{threadId}.log"
          "Path": "app.log"
        }
      ]
    }
  }
}
