{
  "Traceyi": {
    "Tracer": [
      {
        "Listeners": [ "console2", "file" ],
        "Filters": "all"
      }
    ],
    "Listener": {
      "Console": [
        {
          "Name": "console"
        }
      ],
      "File": [
        {
          "Name": "file",
          "Concurrency": 5,
          "UseLock": false,
          "UseAsync": true,
          "UseMutex": false,
          "KeepFilesOpen": true,
          "MaxLength": 25000000,
          "MaxArchiveCount": 0,
          //"CloseTimeout": "0:0:0.200",
          //"Path": "app_{threadId}.log",
          "Path": "app_{index|0,3:R}.log",
          //"Path": "app.log",
          "Layout": {
            //"format": "{action} {elapsed}"
          }
        }
      ]
    }
  }
}
