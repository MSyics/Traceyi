{
  "Traceyi": {
    "Tracer": [
      {
        "Listeners": [ "console", "file" ],
        "Filters": "info",
        "UseMemberInfo": true
      }
    ],
    "Listener": {
      "Console": [
        {
          "Name": "console",
          "UseLock": true,
          "UseAsync": true
        }
      ],
      "File": [
        {
          "Name": "file",
          "UseLock": true,
          "UseAsync": true,
          "MaxLength": 5242880,
          "LeaveFiles": false,
          "Layout": {
            "Format": "{message}"
          },
          "Path": "app.log"
        }
      ]
    }
  }
}
