{
  "Traceyi": {
    "Tracer": [
      {
        "Listeners": [ "console2", "file" ],
        "Filters": "all"
      }
    ],
    "Listener": {
      //"Console": [
      //  {
      //    "Name": "console"
      //  }
      //],
      "File": [
        {
          "Name": "file",
          "Demux": 1,
          "UseLock": false,
          "UseAsync": true,
          "UseMutex": false,
          "KeepFilesOpen": true,
          "MaxLength": 25000000,
          "MaxArchiveCount": 0,
          //"CloseTimeout": "0:0:0.200",
          //"Path": "app_{index}.log",
          "Path": "app.log",
          "Layout": {
            "format": "{action} {elapsed} {@[action, extensions]=>json}"
          }
        }
      ]
    }
  }
}
