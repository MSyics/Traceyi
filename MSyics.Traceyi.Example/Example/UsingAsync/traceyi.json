{
  "Traceyi": {
    "Tracer": [
      {
        "Listeners": [ "file" ],
        "Filters": "all"
      }
    ],
    "Listener": {
      "Console": [
        {
          "Name": "console",
          "UseLock": false,
          "UseAsync": true
        }
      ],
      "File": [
        {
          "Name": "file",
          "UseLock": false,
          "UseAsync": true,
          "UseMutex": false,
          "KeepFilesOpen": true,
          "MaxLength": 25000000,
          "MaxArchiveCount": 1,
          "Path": "app.log",
          "layout": {
            "format": "{@}"
          }
        }
      ]
    }
  }
}
