{
  "Traceyi": {
    "Tracer": [
      {
        "Listeners": [ "console1", "file" ],
        "Filters": "all"
      }
    ],
    "listener": {
      "console": [
        {
          "Name": "console",
          "layout": {
            "format": "{action} {elapsed} {@[action, extensions]=>json}"
          }
        }
      ],
      "file": [
        {
          "name": "file",
          "Demux": 1,
          "UseLock": false,
          "UseAsync": true,
          "UseMutex": false,
          "KeepFilesOpen": true,
          "MaxLength": 25000000,
          "MaxArchiveCount": 0,
          //"CloseTimeout": "0:0:0.200",
          //"Path": "app_{index}.log",
          "Path": "app.log",
          "layout": {
            //"format": "{action} {elapsed} {@[action, extensions]=>json}"
            //"format": "     {@=>json,indent}\n\n\n"
            //"format": "{action| ,8:R}\n"
          }
        }
      ]
    }
  }
}
