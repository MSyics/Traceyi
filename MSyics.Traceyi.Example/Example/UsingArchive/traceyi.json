{
  "Traceyi": {
    "Tracer": [
      {
        "Listeners": [ "file", "console" ],
        "Filters": "all"
      },
      {
        "Name":  "second",
        "Listeners": [ "file2", "console" ],
        "Filters": "all"
      }
    ],
    "Listener": {
      "Console": [
        {
          "Name": "console"
        }
      ],
      "File": [
        {
          "Name": "file",
          "Concurrency": 2,
          "UseLock": false,
          "UseAsync": true,
          "UseMutex": true,
          "KeepFilesOpen": true,
          "MaxLength": 10240,
          "MaxArchiveCount": 10000,
          "Path": "UsingArchive\\app.log"
        },
        {
          "Name": "file2",
          "Concurrency": 2,
          "UseLock": false,
          "UseAsync": true,
          "UseMutex": true,
          "KeepFilesOpen": true,
          "MaxLength": 10240,
          "MaxArchiveCount": 10000,
          "Path": "UsingArchive\\app.log"
        }
      ]
    }
  }
}
