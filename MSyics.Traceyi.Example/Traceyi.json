{
  "Traceyi": {
    "Tracer": [
      {
        /* DefaultTracer */
        //"Name":  "",
        //"Filters": "All",
        "UseMemberInfo": true,
        "Listeners": [ "file", "console" ]
      },
      {
        "Name": "test",
        "Listeners": [ "defaultConsole", "defaultFile" ]
      },
      {
        "Name": "messageOnly",
        "Listeners": [ "messageOnlyConsole" ]
      },
      {
        "Name": "UsingCustomTraceListener",
        "Listeners": [ "custom" ]
      },
      {
        "Name": "UsingTraceMethod",
        "Listeners": [ "action message console" ]
      }
    ],
    "Listener": {
      "Console": [
        {
          "Name": "console",
          "UseLock": false,
          "UseAsync": true
          //"NewLine": "\r\n",
          //"Encoding": "utf-8",
          //"UseErrorStream": false,
          //"Layout": {
          //  "Format": "{dateTime:yyyy/MM/dd}{tab}{dateTime:HH:mm:ss.ffff}{tab}{action}{tab}{operationId}{tab}{activityId}{tab}{class}{tab}{member}{tab}{threadId}{tab}{processId}{tab}{processName}{tab}{machineName}{tab}{message}"
          //}
        },
        {
          "Name": "messageOnlyConsole",
          "Layout": {
            "Format": "{message}"
          }
        },
        {
          "Name": "action message console",
          "Layout": {
            "Format": "{action}{tab}{message}"
          }
        }
      ],
      "File": [
        {
          "Name": "file",
          "UseLock": true,
          "UseAsync":  true
          //"NewLine": "\r\n",
          //"Encoding": "UTF-8",
          //"Path": "",
          //"MaxLength": 0,
          //"LeaveFiles": true,
          //"Layout": {
          //  "Format": "{dateTime:yyyy/MM/dd}{tab}{dateTime:HH:mm:ss.ffff}{tab}{action}{tab}{operationId}{tab}{activityId}{tab}{class}{tab}{member}{tab}{threadId}{tab}{processId}{tab}{processName}{tab}{machineName}{tab}{message}",
          //}
        }
      ],
      "Custom": [
        {
          "Name": "custom"
        }
      ]
    }
  }
}
