{
  "Traceyi": {
    "Tracer": [
      {
        "Listeners": [ "console" ]
      }
    ],
    "Listener": {
      "Console": [
        {
          "Name": "console",
          "Layout": {
            "Format": "{dateTime:yyyy/MM/dd} {dateTime:HH:mm:ss.ffff}{tab}{action}{tab}{operationId}{tab}{activityId}{tab}{member}{tab}{threadId}{tab}{message}"
          }
        }
      ]
    }
  }
}
